{
  "name": "graphql-fullstack-app",
  "version": "1.0.0",
  "scripts": {
    "start:client": "cd client && react-scripts start",
    "build:client": "cd client && react-scripts build",
    "test:client": "cd client && react-scripts test",
    "eject:client": "cd client && react-scripts eject",
    "start:server": "cd server && node index.js",
    "dev:server": "cd server && ../node_modules/nodemon/bin/nodemon.js index.js",
    "start": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run start:client\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "2.0.7"
  }
}
