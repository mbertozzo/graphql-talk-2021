    "lodash": "^4.17.21",
    "mysql2": "^2.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^6.0.0",
    "css-loader": "^2.0.0",
